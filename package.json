{
  "name": "nuxtcomponentpopdoc",
  "publisher": "jptech",
  "displayName": "NuxtComponentPopDoc",
  "description": "Add popup documentation for Nuxt components when hovering over them.",
  "license": "MIT",
  "icon": "img/icon.png",
  "sponsor": {
    "url": "https://github.com/sponsors/johnpuaoi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnpuaoi/nuxtcomponentpopdoc.git"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nuxtcomponentpopdoc.createPopDocComment",
        "title": "Create PopDoc Comment"
      }
    ],
    "keybindings": [
      {
        "command": "nuxtcomponentpopdoc.createPopDocComment",
        "key": "<pd"
      }
    ],
    "configuration": {
      "title": "Nuxt Component Pop Doc",
      "properties": {
        "nuxtcomponentpopdoc.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable Nuxt Component Pop Doc."
        },
        "nuxtcomponentpopdoc.commentTemplate": {
          "type": "string",
          "default": "<!-- @PopDoc \n # Component Name \n A description of the component, how it works, and what it's used for. Describe @important things that the user should know about the component. \n \n ## Props \n /** \n  * The `exampleProp` prop is an example of a prop that you can pass to the component. Use this format to describe the prop. \n  */ \n exampleProp?: string; \n \n ## Emits \n - exampleEmit: (value: any) => void. emitted when an example event happens \n \n ## Slots \n - example list of slots \n \n ## Examples \n ```vue \n<script lang=\"ts\" setup> \n \n</script> \n \n<template>\n <div>Test</div> \n</template> \n ``` \n \n --> \n",
          "description": "Template used for creating PopDoc comments.",
          "editPresentation": "multilineText"
        },
        "nuxtcomponentpopdoc.commentInsertKeys": {
          "type": "string",
          "default": "<pd",
          "description": "Key combination used to insert PopDoc comment."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "vsce": "^2.11.0"
  }
}
